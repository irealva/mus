//*Farbound*//
//a note can have different durations
Note note1 = new Note(C, 4, 2);
Sequence sequence = new Sequence() ;

//and their durations can be modified to the following duration
note1^2;
sequence << note1 ;
note1^4;
sequence << note1 ;
note1^8;
sequence << note1 ;
note1^16;
sequence << note1 ;
note1^32;
sequence << note1 ;
note1^64;
sequence << note1 ;
note1^128;
sequence << note1 ;


display(sequence, "Testing Note Duration") ;

