//*Farbound*//
//a note can have different shapes
Note note1 = new Note(C, 4, 8);
Sequence sequence = new Sequence() ;

//with the following shapes
note1 < Shape('default');
sequence << note1 ;
note1 < Shape('altdefault');
sequence << note1 ;
note1 < Shape('baroque');
sequence << note1 ;
note1 < Shape('neomensural');
sequence << note1 ;
note1 < Shape('mensural');
sequence << note1 ;
note1 < Shape('petrucci');
sequence << note1 ;
note1 < Shape('harmonic');
sequence << note1 ;
note1 < Shape('harmonic-black');
sequence << note1 ;
note1 < Shape('harmonic-mixed');
sequence << note1 ;
note1 < Shape('diamond');
sequence << note1 ;
note1 < Shape('cross');
sequence << note1 ;
note1 < Shape('xcircle');
sequence << note1 ;
note1 < Shape('triangle');
sequence << note1 ;
note1 < Shape('slash');
sequence << note1 ;

display(sequence, "Testing Note Shapes") ;
